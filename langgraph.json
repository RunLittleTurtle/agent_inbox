{
  "dependencies": ["./src", "./src/executive-ai-assistant"],
  "graphs": {
    "agent": "./src/graph.py:graph",
    "executive_main": "./src/executive-ai-assistant/eaia/main/graph.py:app",
    "executive_cron": "./src/executive-ai-assistant/eaia/cron_graph.py:graph",
    "executive_general_reflection": "./src/executive-ai-assistant/eaia/reflection_graphs.py:general_reflection_graph",
    "executive_multi_reflection": "./src/executive-ai-assistant/eaia/reflection_graphs.py:multi_reflection_graph"
  },
  "env": ".env",
  "python_version": "3.13",
  "dockerfile_lines": [
    "ENV PYTHONIOENCODING=UTF-8",
    "ENV PYTHONUTF8=1",
    "ENV LANG=C.UTF-8",
    "ENV LC_ALL=C.UTF-8"
  ],
  "store": {
    "index": {
      "embed": "openai:text-embedding-3-small",
      "dims": 1536,
      "fields": ["$"]
    },
    "ttl": {
      "strategy": "last_accessed",
      "days": 90
    }
  },
  "checkpoints": {
    "ttl": {
      "strategy": "delete",
      "sweep_interval_minutes": 60,
      "default_ttl": 43200
    }
  },
  "auth": {
    "path": "./src/auth.py:auth"
  }
}
